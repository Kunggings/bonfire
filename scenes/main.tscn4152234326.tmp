[sub_resource type="GDScript" id="GDScript_muem4"]
script/source = "extends Node

var total_enemies: int = 0

@export var max_enemies: int = 20
@export var min_spawn_dis: int = 300
@export var max_spawn_dis: int = 500
@export var player: Player
@export var enemy_spawn_rate: float = 2.0

@onready var enemy_spawn_timer: Timer = $\"Enemy Spawn Timer\"

func _ready() -> void:
	enemy_spawn_timer.wait_time = enemy_spawn_rate
	enemy_spawn_timer.timeout.connect(spawn_enemy)
	enemy_spawn_timer.start()

func spawn_enemy():
	if total_enemies >= max_enemies:
		return

	total_enemies += 1

	var dir := Vector2.RIGHT.rotated(randf_range(0.0, TAU))
	var distance := randf_range(min_spawn_dis, max_spawn_dis)
	var spawn_location := player.position + dir * distance

	var enemy = preload(\"res://scenes/enemy.tscn\").instantiate()
	enemy.position = spawn_location
	enemy.target_body = player

	print(\"spawned enemy\", spawn_location)
	add_child(enemy)
"

